meta {
  name: 프로필 사진 업로드 (가입 전)
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/upload/profile/temp
  body: multipartForm
  auth: none
}

body:multipart-form {
  file: @file(profile.jpg)
}

